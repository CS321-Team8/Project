{
"manifest_version": 2,
"name" :"Focus",
"version" :"1.0",
"description" :"Using this extension can help you block social networking reminders when you want to study hard.",
"icons": { "128": "images/icon_black.png" },
"browser_action" : {
  "default_icon": "images/icon_black.png",
	"default_popup": "html/popup.html",
  "default_title": "Focus Mode"
},
"background": {
  "scripts": ["js/background.js"],
  "persistent": true
},
"permissions": [
  "tabs",
  "contentSettings",
  "storage"
]
}
